root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
end_of_line = lf
# editorconfig-tools is unable to ignore longs strings or urls
max_line_length = off

[*.js]
indent_style = space
indent_size = 2

[*.py]
indent_style = space
indent_size = 4

[CHANGELOG.md]
indent_size = false
